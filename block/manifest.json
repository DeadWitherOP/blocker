// manifest.json
{
  "manifest_version": 2,
  "name": "lol",
  "version": "1.0",
  "permissions": [
    "webNavigation",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
